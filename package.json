  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "babel-preset-minify": "^0.5.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.7.2",
    "process": "^0.11.10"
